@model Responce;
@{
    string skud = "skud";
}

<div class="mainimg">
    @for(int i = 17; i != 23; i++)
    {
        skud = $"skud{i}";
        <div id="@skud" class="skud @skud"></div>
    }
    @for (int i = 0; i != 7; i++)
    {
        skud = $"skud{i}";
        <div id="@skud" class="skud @skud"></div>
    }
    @for (int i = 16; i != 6; i--)
    {
        skud = $"skud{i}";
        <div id="@skud" class="skud skudbot @skud"></div>
    }
</div>